    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "lint": "eslint src --ext .tsx --ext .ts",
        "lint:fix": "yarn lint --fix",
        "format": "prettier --config ./.prettierrc -w 'src/**/*.{tsx,ts}' && git update-index --again",
