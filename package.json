        "@reduxjs/toolkit": "^1.8.1",
        "framer-motion": "^6.2.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@types/jest": "^27.4.1",
